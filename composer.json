{
  "name": "karelwintersky/monolog-pdo-handler",
  "description": "A handler for Monolog that stores data via PDO driver to Database",
  "keywords": [
    "log",
    "logging",
    "monolog",
    "pdo",
    "database"
  ],
  "homepage": "https://github.com/KarelWintersky/monolog-pdo-handler",
  "license": "MIT",
  "authors": [
    {
      "name": "Karel Wintersky",
      "email": "karel.wintersky@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-pdo": "*",
    "monolog/monolog": ">1.4.0"
  },
  "autoload": {
    "psr-4": {
      "KarelWintersky\\Monolog\\": "src/"
    }
  },
  "extra": {
    "class": "KarelWintersky\\Monolog\\KWPDOHandler"
  }
}
